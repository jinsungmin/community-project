{
    "name": "community-project",
    "version": "0.0.1",
    "os": [
        "darwin",
        "linux"
    ],
    "engine-strict": true,
    "engines": {
        "node": ">=14"
    },
    "private": true,
    "scripts": {
        "build": " rm -rf ./build && ./node_modules/typescript/bin/tsc && npm run add-assets",
        "add-assets": "copyfiles -u 1 './src/**/!(*.js|*.jsx|*.map|*.ts|*.tsx)' ./build/src",
        "local": "NODE_ENV=test nodemon --exec ts-node --transpile-only ./src/app.ts",
        "dev": "NODE_ENV=development ts-node --transpile-only ./src/app.ts",
        "start": "NODE_ENV=production ts-node --transpile-only ./src/app.ts",
        "deploy": "npm run build && bash scripts/deploy-dev.sh",
        "deploy:production": "npm run build && bash scripts/deploy-prod.sh",
        "lint": "eslint ./src/**/*.ts --fix",
        "test": "NODE_ENV=test mocha"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "{src,test}/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "ajv": "^6.12.6",
        "aws-sdk": "^2.852.0",
        "axios": "^0.21.1",
        "cheerio": "^1.0.0-rc.10",
        "config": "^3.3.3",
        "connect-redis": "^5.0.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "express": "^4.17.1",
        "express-basic-auth": "^1.2.0",
        "express-session": "^1.17.1",
        "express-winston": "^4.1.0",
        "firebase-admin": "^9.4.2",
        "fluent-logger": "^3.4.1",
        "helmet": "^3.23.3",
        "iamport": "^0.3.4",
        "js-combinatorics": "^1.4.5",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mime-types": "^2.1.29",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "morgan": "^1.10.0",
        "mysql": "^2.18.1",
        "nodemailer": "^6.4.17",
        "passport-google-oauth2": "^0.2.0",
        "popbill": "^1.23.0",
        "redis": "^3.0.2",
        "request": "^2.88.2",
        "serialize-error": "^7.0.1",
        "swagger-ui-dist": "^3.44.0",
        "uuid": "^8.3.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/express": "^4.17.11",
        "@types/faker": "^4.1.12",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mocha": "^8.2.1",
        "@types/morgan": "^1.9.2",
        "@types/mysql": "^2.15.17",
        "@types/nodemailer": "^6.4.0",
        "@types/sinon": "^9.0.10",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "chai": "^4.2.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.18.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-chai-expect": "^2.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
        "faker": "^4.1.0",
        "lint-staged": "^10.5.3",
        "mocha": "^8.1.3",
        "node-mocks-http": "^1.10.0",
        "nodemon": "^2.0.7",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "randexp": "^0.5.3",
        "sinon": "^9.2.3",
        "superagent": "6.1.0",
        "supertest": "^5.0.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    }
}
